{
  "name": "snakepipe",
  "version": "0.1.4",
  "description": "A node implementation of the rust snakepipe crate",
  "main": "dist/src/lib.js",
  "types": "dist/src/lib.d.ts",
  "bin": {
    "snakepipe-node": "./bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:zod-schemas": "ts-to-zod ./src/types.ts ./src/schemas.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topheman/snake-pipe-node.git"
  },
  "keywords": [
    "cli",
    "game",
    "snake",
    "unix",
    "pipe",
    "stream",
    "async generator",
    "zod"
  ],
  "author": "Christophe Rosset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topheman/snake-pipe-node/issues"
  },
  "homepage": "https://github.com/topheman/snake-pipe-node#readme",
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "ts-to-zod": "^3.7.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "zod": "^3.22.4"
  }
}
